<h1><%= params[:controller].humanize %> - <%= params[:action].humanize %> - <%= params[:method].humanize %></h1>

<div style="margin-bottom: 1em;">
<%= javascript_tag do %>
  var s = '<%= @result %>';
  document.write(s);
<% end %>
</div>

<div style="margin-bottom: 1em;">
<%= javascript_tag do %>
  var d = "<%= @result %>";
  document.write(d);
<% end %>
</div>

<p>Was @result HTML Safe? <%= @result.html_safe? ? 'Yes' : 'No' %></p>

<%= link_to 'Back', base_test_javascript_helper_form_path(encode_method(params[:method]), params[:option]) %>